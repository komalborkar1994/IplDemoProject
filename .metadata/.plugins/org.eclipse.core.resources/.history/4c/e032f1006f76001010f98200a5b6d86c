package staticprogram;



public class PrivateKeyword {
	
	private	String name;
	
		private int mobno;
		
		private String deparment;
		
		private void printname() {
			System.out.println("komal");
		}
		
		public Void task()
		{
			printname();
			System.out.println("name:"+"komal");
			System.out.println("mobno:"+2343544);
			System.out.println("deparment:"+"bca");
			return null;
		
		}
		
		
		
		
		
	}



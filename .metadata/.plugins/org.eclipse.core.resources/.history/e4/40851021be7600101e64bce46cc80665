package repository;

import entity.Team;

public class Teamrepository {
	public static Team getMITeamDetail() {
	Team team=new Team();
	team.setId(101);
	team.setTeamName("mumbai");
	team.setCaptainName("hardik pandya");
	team.setCoachName("jayvardhane");
	team.setnRR(2.3f);
	team.setQualified(false);
	return team;
	
	}

package com.kk.controllers;

public class LcmProgram {
	public static void main(String[] args) {
		int no1=12;
		int no2=15;
		int lcm=1;
		if(no1>no2)
		{
			lcm=no2;
		}
		for(int i=1;i<=no1&& i<=no2;i++) {
			if(no1%i==0 && no2%i==0)
			{
				lcm=i;
			}
		}
		System.out.println("lcm:"+lcm);
	}
	

}
	}

}

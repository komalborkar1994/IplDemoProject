package constructors;

import java.awt.Color;

public class Animal {
	public void getName() {
		String name="monday";
		System.out.println(name);
	}
	public void getAge(int age)
	{
		age=34;
		System.out.println(age);
	}
	public String getColour() {
		String colour="black";
		return colour;
		
	}
public int getShare(int income)
{
	int share=income/2;
	return share;
}

	

public static void main(String[] args) {
	Animal animal=new Animal();
	int share=animal.getShare(234);
	animal.getAge(12);
	animal.getName();
	String updatename=animal.getUpdateName();
}
}
